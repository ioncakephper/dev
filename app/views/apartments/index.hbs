<h1>Apartments</h1>

<table class="table table-striped table-hover table-sm">
    <thead>
        <tr>
            <th>ID</th>
            <th>Number</th>
            <th>Rooms</th>
        </tr>
    </thead>
    <tbody>
        {{#each records as |record|}}
        <tr>
            <td>{{{record.ID}}}</td>
            <td>{{{record.ID}}}</td>
            <td>{{{record.ID}}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>
